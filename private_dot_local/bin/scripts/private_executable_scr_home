#!/bin/sh
xrandr --output eDP --off --output DisplayPort-0 --off --output DisplayPort-1 --off --output DisplayPort-2 --primary --mode 3440x1440 --pos 0x0 --rotate normal --output DisplayPort-3 --mode 2560x1440 --pos -2560x0 --rotate normal --output DisplayPort-4 --off --output DisplayPort-4 --off
setbg ~/.local/share/bg
setxkbmap -model pc105 -layout dk
